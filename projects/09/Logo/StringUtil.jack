class StringUtil {
    function boolean equals(String s1, String s2) {
        var int i;
        var int sz;
        if (~(s1.length() = s2.length())) {
            return false;
        }
        let sz = s1.length();
        while (i < sz) {
            if (~(s1.charAt(i) = s2.charAt(i))) {
                return false;
            }
            let i = i + 1;
        }
        return true;
    }
}